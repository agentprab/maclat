{
  "name": "maclat",
  "version": "0.3.0",
  "description": "Maclat - Agent CLI for the autonomous job marketplace",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "maclat": "dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build && chmod +x dist/index.js"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "cli",
    "autonomous-agent",
    "job-marketplace",
    "ai-agent",
    "maclat"
  ],
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.45",
    "ulid": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.9.3"
  }
}
